<div class="row">
  <h5>Friends Activity Feed</h5>
</div>
<div clas="row">
  <% @feed.each do |activity| %>
  <div class="divider"></div>
   <div class="section">
     <div class="col s6 m3 profile-column">
       <img class="responsive-img circle" src="http://www.pageresource.com/wallpapers/wallpaper/cats-cute-cat-animal-cute-grass-photo.jpg"></img>
       <p class="grey-text"><%= activity.date %></p>
     </div>
     <div class="col s6 m9 text-column">
       <div class="section username">
         <h5><%= "#{activity.user.first_name} #{activity.user.last_name}" %></h5>
       </div>
       <div class="divider"></div>
       <div class="section activity">
         <span><%= "#{activity.user.first_name} did #{activity.exercise_type.name} at #{activity.intensity.name} intensity for #{activity.num_units} #{activity.exercise_unit.name}" %></span>
         <p class="grey-text"><span><%= activity.cheers.size %> Cheers</span>       <span><%= activity.comments.size %> Comments</span></p>
       </div>
       <div class="divider"></div>
       <%# Need to add way to undo these too  %>
       <div class="section react-buttons">
         <%= link_to '<i class="material-icons">thumb_up</i>', new_exercise_event_cheer_path(activity.id), method: "post" %>
        <%= link_to '<i class="material-icons">comment</i>', new_exercise_event_comment_path(activity.id), method: "post" %>
       </div>
            
       </div>
   
       
     </div>
   </div>
  <% end %>
  <div class="divider"></div>
</div>